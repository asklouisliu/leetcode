class Solution:
    def mySqrt(self, x: int) -> int:
        low, high = -1, x+1
        while (low+1 < high):
            mid = low+(high-low)//2
            square = mid*mid
            if square == x:
                return mid
            if square < x:
                low = mid
            else:
                high = mid
        return low
